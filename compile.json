{
  "targets": [
    {
      "type": "source",
      "outputPath": "source-output"
    },
    {
      "type": "hybrid",
      "outputPath": "hybrid-output"
    },
    {
      "type": "build",
      "outputPath": "build-output"
    }
  ],
  "defaultTarget": "source",
  "locales": ["en"],

  "applications": [
    {
      "class": "osparc.demo.Application",
      "theme": "osparc.theme.OSparc",
      "name": "QxOSparcTheme"
    }
  ],

  "libraries": [
    ".node_modules/qxcompiler/node_modules/qooxdoo-sdk/framework",
    "."
  ]
  ,
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "globals": {
      "osparc": false
    }, 
    "extends": [
      "qx/browser"          
    ] 
  }
}